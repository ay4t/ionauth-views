{
    "name": "ay4t/ionauth-views",
    "description": "Custom Views with IonAuth Libraries for Codeigniter 4",
    "type": "library",
	"license": "MIT",
	"keywords": [
	  "codeigniter", "auth", "codeigniter 4", "ionauth"
	],
	"homepage": "https://github.com/ay4t/ionauth-views",
    "authors": [
        {
            "name": "Ayatulloh Ahad R",
            "email": "ayatulloh@indiega.net"
        }
    ],
	"require-dev": {
        "phpunit/phpunit": "^7.3",
        "codeigniter4/codeigniter4-standard": "^1.0",
        "squizlabs/php_codesniffer": "^3.3",
        "benedmunds/codeigniter-ion-auth": "4.x-dev"
	},
	"autoload": {
		"psr-4" : {
		  "IonauthView\\": "/"
		}
	},
    "repositories": {
        "repo-name": {
            "type": "vcs",
            "url": "git@github.com:ay4t/ionauth-views.git"
        },
        "ionAuth": {
            "type": "vcs",
            "url": "git@github.com:ay4t/ionauth-views.git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
